{
	"training_data": "data/FB15K/",
  	"output": "../../output/results/",
  	"dataset_division": "",

  	"embedding_module": "TransE",
  	"alignment_module": "sharing",
	"search_module": "greedy",
	  
    "dim": 100,
  	"init": "normal",
	"ent_l2_norm": false,
	"rel_l2_norm": false,
	"loss_norm": "L1",
	"margin": 1.5,
	"loss": "margin-based",
	"test_tail": false,
	"neg_sampling": "uniform",
	"neg_triple_num": 2,
	  
	"learning_rate": 0.05,
    "optimizer": "Adagrad",
	"max_epoch": 20,
	"batch_size": 100,

	"batch_threads_num": 2,
	"start_valid": 10,
	"eval_freq": 10,
	"stop_metric": "hits1",
	"top_k": [1, 5, 10, 50],
	"is_gpu": true,
	"is_parallel": false,
	"device_number": 1,
	"num_worker": 1
}